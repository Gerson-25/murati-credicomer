<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    android:label="Terminos y Condiciones"
    app:startDestination="@id/nav_alliance">

    <fragment
        android:id="@+id/nav_home_travel"
        android:name="sv.com.credicomer.murati.ui.travel.fragments.HomeTravelFragment"
        android:label="@string/menu_travel"
        tools:layout="@layout/fragment_home_travel">
        <argument
            android:name="id"
            app:argType="string" />
        <argument
            android:name="date"
            app:argType="string" />
        <action
            android:id="@+id/action_nav_home_to_travelRegistrationFragment"
            app:destination="@id/travelRegistrationFragment"
            app:popUpToInclusive="false" />
        <argument
            android:name="isTravelActive"
            android:defaultValue="false"
            app:argType="boolean" />
        <action
            android:id="@+id/action_nav_home_travel_to_nav_detailrecord"
            app:destination="@+id/detailRecordFragment"
            app:popUpTo="@id/nav_home_travel"
            app:popUpToInclusive="false" />
        <action
            android:id="@+id/action_nav_home_travel_to_addRecordFragment"
            app:destination="@id/addRecordFragment"
            app:popUpTo="@+id/nav_home_travel" />
        <action
            android:id="@+id/action_nav_home_travel_to_termnsFragment2"
            app:destination="@id/termnsFragment"
            app:popUpTo="@+id/nav_home_travel" />
        <action
            android:id="@+id/action_nav_home_travel_to_nav_home"
            app:destination="@id/nav_home"
            app:popUpTo="@+id/nav_home_travel"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_nav_home_travel_to_nav_home2"
            app:destination="@id/nav_home" />

    </fragment>

    <fragment
        android:id="@+id/nav_map"
        android:name="sv.com.credicomer.murati.ui.ride.fragments.MapFragment"
        android:label="@string/menu_ride"
        tools:layout="@layout/fragment_map" />

    <fragment
        android:id="@+id/nav_alliance"
        android:name="sv.com.credicomer.murati.ui.alliance.fragments.AllianceFragment"
        android:label="@string/menu_alliance"
        tools:layout="@layout/fragment_alliance">
        <action
            android:id="@+id/action_nav_allia_to_establishmentsFragment"
            app:destination="@id/establishmentsFragment"
            app:popUpTo="@+id/nav_alliance" />
        <action
            android:id="@+id/action_nav_allia_to_promotionDetailFragment"
            app:destination="@id/promotionDetailFragment" />
    </fragment>

    <fragment
        android:id="@+id/nav_settings"
        android:name="sv.com.credicomer.murati.ui.share.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_settings">
        <action
            android:id="@+id/action_nav_settings_to_createCarnetFragment"
            app:destination="@id/createCarnetFragment"
            app:popUpTo="@+id/nav_settings" />
    </fragment>

    <fragment
        android:id="@+id/nav_ride"
        android:name="sv.com.credicomer.murati.ui.ride.fragments.RideFragment"
        android:label="@string/menu_ride"
        tools:layout="@layout/fragment_ride">
        <argument
            android:name="record"
            app:argType="sv.com.credicomer.murati.ui.travel.models.Record" />
    </fragment>
    <fragment
        android:id="@+id/nav_home"
        android:name="sv.com.credicomer.murati.ui.travel.fragments.HomeFragment"
        android:label="Historial de Viajes">
        <argument
            android:name="isTravelActive"
            android:defaultValue="false"
            app:argType="boolean" />
        <action
            android:id="@+id/action_nav_home_to_nav_home_travel"
            app:destination="@id/nav_home_travel"
            app:popUpTo="@+id/nav_home" />
        <action
            android:id="@+id/action_nav_home_to_termnsFragment"
            app:destination="@id/termnsFragment"
            app:popUpTo="@+id/nav_home" />
        <action
            android:id="@+id/action_nav_home_to_travelRegistrationFragment2"
            app:destination="@id/travelRegistrationFragment"
            app:popUpTo="@id/nav_home" />
    </fragment>
    <fragment
        android:id="@+id/travelRegistrationFragment"
        android:name="sv.com.credicomer.murati.ui.travel.fragments.TravelRegistrationFragment"
        android:label="Nuevo Viaje">
        <argument
            android:name="id"
            app:argType="string" />
        <argument
            android:name="date"
            app:argType="string" />
        <action
            android:id="@+id/action_travelRegistrationFragment_to_nav_home"
            app:destination="@id/nav_home_travel"
            app:popUpTo="@id/nav_alliance"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/establishmentsFragment"
        android:name="sv.com.credicomer.murati.ui.alliance.fragments.EstablishmentsFragment"
        android:label="Establecimientos">
        <action
            android:id="@+id/action_establishmentsFragment_to_promotionsFragment"
            app:destination="@id/promotionsFragment"
            app:popUpTo="@+id/establishmentsFragment" />
        <argument android:name="idCategory" />
    </fragment>
    <fragment
        android:id="@+id/promotionsFragment"
        android:name="sv.com.credicomer.murati.ui.alliance.fragments.PromotionsFragment"
        android:label="Promociones">
        <argument
            android:name="establishment"
            app:argType="sv.com.credicomer.murati.ui.alliance.models.EstablishmentFS" />
        <argument
            android:name="idCategory"
            app:argType="string" />
        <action
            android:id="@+id/action_promotionsFragment_to_promotionDetailFragment"
            app:destination="@id/promotionDetailFragment"
            app:popUpTo="@+id/promotionsFragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_ridefs"
        android:name="sv.com.credicomer.murati.ui.ride.fragments.ReservationFSFragment"
        android:label="ReservationFSFragment" />
    <fragment
        android:id="@+id/addRecordFragment"
        android:name="sv.com.credicomer.murati.ui.travel.fragments.AddRecordFragment"
        android:label="Nuevo Gasto">
        <argument
            android:name="idTravel"
            app:argType="string" />
        <argument
            android:name="record"
            app:argType="sv.com.credicomer.murati.ui.travel.models.Record" />
        <action
            android:id="@+id/action_addRecordFragment_to_nav_home_travel"
            app:destination="@id/nav_home_travel"
            app:popUpTo="@id/nav_home_travel"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/termnsFragment"
        android:name="sv.com.credicomer.murati.ui.travel.fragments.TermnsFragment"
        android:label="Terminos y Condiciones de uso" />
    <fragment
        android:id="@+id/detailRecordFragment"
        android:name="sv.com.credicomer.murati.ui.travel.fragments.DetailRecordFragment"
        android:label="Detalles de Gasto">
        <argument
            android:name="record"
            app:argType="sv.com.credicomer.murati.ui.travel.models.Record" />
        <argument
            android:name="id"
            app:argType="string" />
        <action
            android:id="@+id/action_detailRecordFragment_to_addRecordFragment"
            app:destination="@id/addRecordFragment"
            app:popUpTo="@+id/detailRecordFragment" />
    </fragment>
    <fragment
        android:id="@+id/promotionDetailFragment"
        android:name="sv.com.credicomer.murati.ui.alliance.fragments.PromotionDetailFragment"
        android:label="Detalles de la promocion">
        <argument
            android:name="promotion"
            app:argType="sv.com.credicomer.murati.ui.alliance.models.PromotionFS" />
        <action
            android:id="@+id/action_promotionDetailFragment_to_createCarnetFragment"
            app:destination="@id/createCarnetFragment" />
    </fragment>
    <fragment
        android:id="@+id/rideSlidingFragment"
        android:name="sv.com.credicomer.murati.ui.ride.fragments.RideSlidingFragment"
        android:label="Ride" />
    <fragment
        android:id="@+id/nav_rooms2"
        android:name="sv.com.credicomer.murati.ui.roomsv2.fragments.RoomsHomeFragment"
        android:label="Room">
        <action
            android:id="@+id/action_nav_rooms2_to_roomDetailFragment"
            app:destination="@id/roomDetailFragment" />
        <action
            android:id="@+id/action_nav_rooms2_to_myReservationsFragment"
            app:destination="@id/myReservationsFragment" />
    </fragment>
    <fragment
        android:id="@+id/createCarnetFragment"
        android:name="sv.com.credicomer.murati.ui.alliance.fragments.CreateCarnetFragment"
        android:label="Carnet">
        <action
            android:id="@+id/action_createCarnetFragment_to_nav_alliance"
            app:destination="@id/nav_alliance"
            app:popUpTo="@+id/createCarnetFragment"
            app:popUpToInclusive="true" />
        <argument
            android:name="settings"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/roomDetailFragment"
        android:name="sv.com.credicomer.murati.ui.roomsv2.fragments.RoomDetailFragment"
        android:label="Detalle de sala">
        android:label="Detalles de la Sala">
        <argument
            android:name="room"
            app:argType="sv.com.credicomer.murati.ui.roomsv2.models.Room" />
        <action
            android:id="@+id/action_roomDetailFragment_to_historyRoomFragment"
            app:destination="@id/historyRoomFragment" />
        <argument
            android:name="date"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/myReservationsFragment"
        android:name="sv.com.credicomer.murati.ui.roomsv2.fragments.MyReservationsFragment"
        android:label="Mis Reservas">
        <action
            android:id="@+id/action_myReservationsFragment_to_roomDetailFragment"
            app:destination="@id/roomDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/historyRoomFragment"
        android:name="sv.com.credicomer.murati.ui.roomsv2.fragments.HistoryRoomFragment"
        android:label="Historial de Sala" >
        <argument
            android:name="NameRoom"
            app:argType="string" />
        <argument
            android:name="Reservations"
            app:argType="sv.com.credicomer.murati.ui.roomsv2.models.RoomDetail"
            app:nullable="true" />
        <argument
            android:name="reservationDate"
            app:argType="string" />
        <argument
            android:name="roomsId"
            app:argType="string" />
    </fragment>
</navigation>